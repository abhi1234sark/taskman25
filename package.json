{
  "name": "task-manager",
  "version": "1.0.0",
  "description": "Full-stack Task Manager with Audit Logs",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "mongoose": "^7.8.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "task-manager",
    "express",
    "react",
    "mongodb"
  ],
  "author": "",
  "license": "MIT"
}
